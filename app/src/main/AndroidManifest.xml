            android:exported="true">

        </activity>
        <activity
            android:name=".Activity.SellerAdmin.VoucherDetailActivity"
            android:parentActivityName=".Activity.SellerAdmin.VoucherShopActivity"

            android:exported="false" />
        <activity
            android:name=".Activity.SellerAdmin.VoucherShopActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".Activity.Voucher.VoucherUserActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".Activity.Order.ChooseVoucherActivity"
            android:parentActivityName=".Activity.Order.OrderActivity"
            android:exported="true" />

        <activity
            android:name=".Activity.Order.OrderAddressActivity"
            android:exported="true" />
        <activity
            android:name=".Activity.Order.AddNewOrderAddressActivity"
            android:exported="true">

