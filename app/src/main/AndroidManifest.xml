        <activity

            android:name=".Activity.Order.OrderActivity"
            android:parentActivityName=".Activity.Order.CartActivity"
            android:exported="false"/>

        </activity>
        <activity
            android:name=".Activity.Product.ProductDetailActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Order.CartActivity"
            android:exported="true">

        </activity>
       
        <activity
            android:name=".Activity.OrderList.OrderListActivity"
            android:exported="true" />

        <activity
            android:name=".Activity.Profile.ProfileActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".Activity.Home.HomeActivity"
            android:exported="true" />
